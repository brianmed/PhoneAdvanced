      <form id="infovoice_form" class="form-horizontal well" method=post>
        <fieldset>
          <legend>InfoVoice</legend>

          <div class="control-group">
            <label class="control-label" for="email">CallerID Number</label>
            <div class="controls">
              <input type="text" class="input-medium" name=internal_nbr value="[% internal_nbr %]" disabled> </label>
            </div>
          </div>

          <div class="control-group">
            <label class="control-label" for="email">Test Number</label>
            <div class="controls">
              <input type="text" class="input-medium" name=test_nbr value="[% test_nbr %]"> </label>
            </div>
          </div>

          <div class="control-group">
            <label class="control-label" for="email">Name</label>
            <div class="controls">
              <input type="text" class="input-xlarge" id="voice_name" name=voice_name value=[% voice_name %]>
            </div>
          </div>

          <div class="control-group">
            <label class="control-label" for="email">InfoVoice</label>
            <div class="controls">
              <textarea class="input-xlarge" id="infovoice" name="infovoice" rows="6">[% infovoice %]</textarea>
            </div>
          </div>

          <div class="form-actions">
            <button id="btntest" type="button" class="btn btn-primary">Test</button>
            <button type="reset" class="btn">Cancel</button>
          </div>
        </fieldset>
      </form>
